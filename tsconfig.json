{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": true,
        "noImplicitAny": false,
        "outDir": "lib/modules",
        "declaration": true
    },
    "exclude": [
        "node_modules",
        "typings",
        "lib"
    ]
}
